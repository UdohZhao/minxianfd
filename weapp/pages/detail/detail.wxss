page,
.page{
  background-color: #f1f1f1;
  width: 100%;
  height: 100%;
}
.swiper {
  height: 600rpx;
  width: 100%;
}
.swiper image {
  height: 100%;
  width: 100%;
}
.weui-cells__title{
  background-color: #fff;
  height: 40px;
  line-height: 40px;
  margin-top: 0;
  color: #333;
  font-size: 20px;
}
.weui-panel{
  padding:15px;
  margin-bottom: .3rem;
}
.weui-panel_header{
  font-size: 16px;
  color: #ff8900;
  line-height: 35px;
  height: 35px;
  overflow: hidden;
}
.weui-panel_list{
  flex: 1;
  width: 50%;
  display: inline-block;
  line-height: 2;
}
.weui-panel_list_val{
  flex: 1;
  display: inline-block;
  width: 60%;
  text-indent: 3px;
}
.weui-panel_list_key{
  width: 40%;
  display: inline-block;
  color: #999;
  font-size: 13px;
}

.weui-panel_title{
  color: #00C777;
  padding-bottom: 10px;
  border-bottom: 1px solid #eee;
}
.weui-panel__bd{
  padding: 10px 0;
}
.weui-panel___bd_desc{
  flex: 1;
  display: inline-block;
  width: 33.333%;
  line-height: 2;
}
.weui-panel___bd_desc image{
  display: inline-block;
  width: 20px;
  height: 20px;
  vertical-align: middle;
}
.desc{
  display: inline-block;
  text-indent: 3px;
  vertical-align: middle;
  font-size: 13px;
}
.fs13{
  font-size: 13px;
}
.h60{
  width: 100%;
  height: 60px;
  clear: both;
  content: '';
  font-size: 0;
}
.footerModel{
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 60px;
  background-color: #f3f3f3;
  overflow: hidden;
  flex: 1;
  border-top: 1px solid #eee;
}
.modelImg{
  float: left;
  width: 20%;
  text-align: center;
  padding: 10px 0;
  height: 60px;
  box-sizing: border-box;
  
}
.modelImg image{
  display: inline-block;
  width: 40px;
  height: 40px;
}
.modelCenter{
  float: left;
  width: 50%;
  text-align: center;
  background-color: #ff8900;
  color: #fff;
  font-size: 14px;
  height: 60px;
}
.modelCenter view{
  height: 30px;
  line-height: 30px;
}
 .modelPhone{
  float:right;
  width: 30%;
  background-color: #00C777;
  color: #fff;
  height: 60px;
  padding: 10px 0;
  vertical-align: text-bottom;
  box-sizing: border-box;
  text-align: center;
} 
.modelPhone image{
  display: inline-block;
  width: 30px;
  height: 30px;
  margin-right: 10px;
  vertical-align: text-bottom;
}
