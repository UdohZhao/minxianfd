.page_weui_hd{
  position: fixed;
  top:0;
  left: 0;
  width: 100%;
  height: 30px;
  line-height: 30px;
  text-align: center;
  color: #ff8900;
  border-bottom: 1px solid #ddd;
  background-color: #fff;
  z-index: 2;
}
.marTop30{
  margin-top: 30px;
}
.page_bd_box_tiitle{
  padding:0 15px;
  height: 40px;
  line-height: 40px;
}
.titleTime{
  color: #333;
  vertical-align: middle;
  display: inline-block;
}
.close{
  float: right;
  font-size: 20px;
  color: #999;
  vertical-align: middle;
}
.weui-media-box__desc{
  width: 100%;
  height: 20px;
  line-height: 20px;
  color: #333;
  font-weight: 600;
}

.weui-media-box::before{
  border: 0;
}
.weui-media-box{
  border-bottom:0.5px solid #e5e5e5; 
}
.errorMsg{
  width: 100%;
  height: 200px;
  line-height: 200px;
  text-align: center;
  font-size: 16px;
}
/* 信息列表标签 */
.weui-media-box__hd_in-appmsg{
  width: 100px;
  height: 100px;
  line-height: 100px;
}
.weui-media-box__desc{
  width: 100%;
  height: 23px;
  line-height: 1.6;
  color: #333;
  font-size: 12px;
}
.red{
  font-size: 16px;
  color: #f00;
}
.lebalSty{
  padding: 3px;
  margin-right: 5px;
  font-size: 10px;
  color: #fff;
}
.lebalStyleDT{
  background-color: #DEF2EB;
  color: #32B983;
}
.lebalStyleSSKF{
  background-color: #F8F4EA;
  color: #F79B2A;
}
.lebalStyleXSFY{
  background-color: #E2F4FF;
  color: #6EA9DC;
}
.lebalStyleJZX{
  background-color: #F5DECB;
  color: #D0845D;
}
.lebalStyleYCW{
