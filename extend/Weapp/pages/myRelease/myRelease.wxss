page,
.page{
  background-color: #f1f1f1;
  width: 100%;
}
.page_weui_hd{
  position: fixed;
  top:0;
  left: 0;
  width: 100%;
  height: 30px;
  line-height: 30px;
  text-align: center;
  background-color: #fff;
  color: #ff8900;
  z-index: 2;
}
.marTop30{
  margin-top: 30px;
}
.rt{
  float: right;
}
.red{
  color: #f00;
}
.page__bd{
  background-color: #fff;
  border-bottom: 1px solid #eee;
  border-top:1px solid #eee;
}
.weui-media-box__desc{
  line-height: 30px;
  height: 30px;
}
.color333{
  color: #333;
}
.green{
  color: green;
}
.errorMsg{
  width: 100%;
  height: 200px;
  line-height: 200px;
  text-align: center;
  font-size: 16px;
}


.box__titleLabel{
  float: right;
  font-size: 12px;
  line-height: 1.8;
  color:#f00;
}
.Notplaced{
  color: #999;
}
.btnRight{
  text-align: right;
}
button[size="mini"]{
  line-height: 1.8;
  color: #999;
}
.weui-media-box__bd_in-appmsg{
  position: relative;
}
.NotPay{
  position: absolute;
  bottom: 0;
  right:0;
}
.weui-media-box_title{
  width: 100%;
  height: 35px;
  line-height: 35px;
  text-align: right;
  vertical-align: middle; 
}
.weui-media-box__title_left{
  float: left;
  padding-left: 15px;
  width: 50%;
  overflow: hidden;
  text-align: left;
  vertical-align:middle;
}
button[size="mini"]{
  color: #fff;
}
.reviewRemark{
  display: inline-block;
  margin-right: 10px;
}
.weui-media-box__title{
  margin-top: 2%;
}
button[type="default"]{
  color: #333;
}


















